@model IEnumerable<Asm_ph56371.Models.NguoiDung>

@{
    ViewData["Title"] = "Quản lý người dùng";
}

<h2>Quản lý người dùng</h2>

<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>Vai trò</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@(user.VaiTro == 1 ? "Admin" : "Khách")</td>
                
            </tr>
        }
    </tbody>
</table>
